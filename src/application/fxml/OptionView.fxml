               <Image url="@../assets/images/animals/fish.jpg" />
            </image>
         </ImageView>
         <ImageView fx:id="Imageview_fruit" fitHeight="266.0" fitWidth="271.0" layoutX="607.0" layoutY="227.0" onMouseClicked="#Imageview_fruit_click" pickOnBounds="true">
            <image>
               <Image url="@../assets/images/fruits/grape.jpg" />
